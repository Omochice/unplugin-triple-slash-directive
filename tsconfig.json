{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "es2017",
		"module": "NodeNext",
		"lib": ["esnext", "DOM"],
		"moduleResolution": "NodeNext",
		"esModuleInterop": true,
		"strict": true,
		"strictNullChecks": true,
		"resolveJsonModule": true,
		"noUncheckedIndexedAccess": true,
		"noEmit": true,
		"skipLibCheck": true
	},
	"include": ["src"]
}
